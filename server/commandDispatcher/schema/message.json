{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "message",
  "type": "object",
  "allOf": [{"$ref": "/command"}],
  "properties": {
    "message": {
      "type": "string",
      "description": "content of the message"
    },
    "success": {
      "type": "boolean",
      "description": "state of the transmission"
    }
  },
  "required": ["message", "success"]
}
