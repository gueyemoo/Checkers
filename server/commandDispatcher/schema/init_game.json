{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "init_game",
  "type": "object",
  "allOf": [{"$ref": "/command"}],
  "properties": {
    "user_id": {
      "type": "string",
      "description": "user id in database"
    },
    "user_color": {
      "type": "number",
      "minimum": 1,
      "maximum": 2,
      "description": "user color"
    }
  },
  "required": ["user_id", "user_color"]
}
